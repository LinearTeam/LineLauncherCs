<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="LMCUI.Controls.LineSplashScreenContent">
    <StackPanel Orientation="Vertical" MaxWidth="500" HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="5">
        <Image Source="/Assets/LMC.png" MaxHeight="400" MaxWidth="400"/>
        <Label Name="pl" HorizontalContentAlignment="Center"></Label>
        <ProgressBar Name="pb"/>
    </StackPanel>
</UserControl>
